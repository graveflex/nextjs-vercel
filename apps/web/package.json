{
  "name": "@mono/web",
  "description": "GFX Monorepo Web App",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf -- ./.turbo ./dist ./coverage ./node_modules ./next",
    "dev": "export $(grep -e '^NEXT_PORT' -- ../../.env) && next dev -p ${NEXT_PORT}",
    "build": "next build",
    "start": "export $(grep -e '^NEXT_PORT' -- ../../.env) && next start -p ${NEXT_PORT}",
    "types": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "generate:types": "payload generate:types",
    "generate:import": "payload generate:importMap",
    "migrations:create": "payload migrate:create",
    "migrations:run": "if [ \"$CI\" != \"true\" ]; then payload migrate; fi",
    "payload": "payload",
    "env:dev": "dotenvx run -f ../../.env.local -f ../../.env",
    "env:ci": "dotenvx run -f ../../.env.local -f ../../.env.ci -f ../../.env.test -f ../../.env",
    "db:setup": "./bin/init_db.sh",
    "db:migrate": "payload migrate",
    "seed:kitchenSink": "pnpm env:dev -- tsx ./bin/seed_kitchen-sink.ts",
    "seed:nav": "pnpm env:dev -- tsx ./bin/seed-nav.ts",
    "seed:blog": "pnpm env:dev -- tsx ./bin/seed_blog.ts",
    "seed:all": "pnpm env:dev -- pnpm seed:nav && pnpm seed:blog && pnpm seed:kitchenSink"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.10.1",
    "@faker-js/faker": "^8.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@mono/theme": "workspace:*",
    "@mono/tsconfig": "workspace:*",
    "@mono/types": "workspace:*",
    "@mono/ui": "workspace:*",
    "@payload-enchants/translator": "latest",
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/db-vercel-postgres": "latest",
    "@payloadcms/email-nodemailer": "latest",
    "@payloadcms/live-preview": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/plugin-cloud-storage": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-vercel-blob": "latest",
    "@payloadcms/translations": "latest",
    "@payloadcms/ui": "latest",
    "@refract-ui/hook-fields": "latest",
    "@refract-ui/sc": "latest",
    "@refract-ui/stories": "latest",
    "@storybook/react": "latest",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@types/escape-html": "^1.0.4",
    "@types/md5": "^2.3.5",
    "@types/node": "^18.17.14",
    "@types/qs": "^6.9.15",
    "@types/ramda": "^0.30.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/styled-components": "latest",
    "@types/tinycolor2": "^1.4.6",
    "@types/tmp": "^0.2.6",
    "@types/voca": "^1.4.5",
    "@vercel/blob": "^0.16.1",
    "@vercel/speed-insights": "^1.0.9",
    "@vitejs/plugin-react": "latest",
    "babel-plugin-react-compiler": "latest",
    "bson-objectid": "^2.0.4",
    "cross-env": "^7.0.3",
    "escape-html": "^1.0.3",
    "framer-motion": "latest",
    "graphql-playground-middleware-express": "^1.7.23",
    "md5": "^2.3.0",
    "next": "latest",
    "next-intl": "3.17.3",
    "nodemailer": "^6.9.14",
    "payload": "latest",
    "payload-cloud-storage-vercel-adapter": "^0.1.5",
    "qs": "^6.11.2",
    "ramda": "^0.30.0",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.51.3",
    "react-loader-spinner": "^6.1.6",
    "sharp": "0.33.3",
    "styled-components": "latest",
    "tinycolor2": "^1.6.0",
    "tmp": "^0.2.3",
    "tsx": "latest",
    "typescript": "latest",
    "vite-plugin-svgr": "latest",
    "vite-transform-stub": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest",
    "vitest-canvas-mock": "latest",
    "voca": "^1.4.1"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.15"
  }
}
